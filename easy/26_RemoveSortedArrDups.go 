package easy 

func removeDuplicates(nums []int) int {
    if len(nums) == 0{
        return 0
    }
    writeIdx, dups := 1, 0
    for i := 1; i < len(nums){
        if nums[i] == nums[i-1] {
            continue
        }
        nums[writeIdx] = nums[i]
        writeIdx++
    }
    return writeIdx
}